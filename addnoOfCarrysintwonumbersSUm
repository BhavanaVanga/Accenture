
  #include <iostream>

using namespace std;
int main()
{
    int n1,n2;
    cin>>n1>>n2;
    int rem1=0,rem2=0,sum=0,rem=0,rem3=0,c=0;
    while(n1>0||n2>0)
    {
       rem1=n1%10;
       rem2=n2%10;
       sum=rem1+rem2+rem;
       if(sum>9)
       {
           rem=sum/10;
           c+=1;
       }
           n1=n1/10;
           n2=n2/10;
    }
    cout<<c;
    return 0;
}
