#include <bits/stdc++.h>
using namespace std;
long long int printMinimumProduct(int arr[], int n) {
        sort(arr,arr+n);
        long long m=arr[0];
        long long k=arr[1];
        return (m*k);
    }
int main() {
	
	int n;
	cin>>n;
	int v[n];
	for(int i=0;i<n;i++)
	{
		cin>>v[i];
	}
	cout<<printMinimumProduct(v,n);
	return 0;
}
